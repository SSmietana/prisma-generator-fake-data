generator client {
  provider = "prisma-client-js"
}

generator custom_generator {
  provider = "node ../../node_modules/prisma-generator-fake-data/dist/bin.js"
  
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id       String        @id @default(cuid())
  email    String        @unique
  name     String
  status   UserStatus
}

enum UserStatus {
  active
  inactive
}